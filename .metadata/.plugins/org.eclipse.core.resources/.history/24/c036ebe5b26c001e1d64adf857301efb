@WebServlet("/GetServlet")
public class GetServlet extends HttpServlet{
	private static final long serialVersionUID=1L;
	
	protected void doGet(HttpServleRequest request, HttpServleResponse response) throws ServleException, IOExceptionp{
	request.setCharacterEncoding("UTF-8");
	String data1 = request.getParmeter("date1");
	String []date2 = request.getParameterValues("date2");
	response.setContentType("text/html; charset=UTF-8");
	PrintWriter out =response.getWriter();
	out.println("<html>");
	out.println("<body>");
	out.println("datal값은 : <br>");
	}

}
